let browser = "Chrome";

switch (browser) {
  case "Chrome":
    console.log("Supported: Best experience in Chrome.");
    break;
  case "Firefox":
    console.log("Supported: Firefox is fully compatible.");
    break;
  case "Safari":
    console.log("Supported: Safari works well.");
    break;
  case "Edge":
    console.log("Supported: Edge support is available.");
    break;
  default:
    console.log("Browser not officially supported.");
}